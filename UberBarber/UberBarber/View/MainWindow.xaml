<Window x:Class="UberBarber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UberBarber"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button_user" Content="User" HorizontalAlignment="Left" Margin="344,226,0,0" VerticalAlignment="Top" Height="38" Width="88" Click="button_user_Click"/>
        <Button x:Name="button_service" Content="Service" HorizontalAlignment="Left" Margin="344,182,0,0" VerticalAlignment="Top" Height="39" Width="88"/>
        <Button x:Name="button_barber" Content="Barber" HorizontalAlignment="Left" Margin="344,138,0,0" VerticalAlignment="Top" Height="39" Width="88"/>
        <Button x:Name="appointments_button" Content="appointments" HorizontalAlignment="Left" Margin="344,86,0,0" VerticalAlignment="Top" Height="39" Width="88" Click="appointments_button_Click"/>
    </Grid>
</Window>
