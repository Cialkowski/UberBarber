<Window x:Class="UberBarber.View.AddAppointment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UberBarber.View"
        mc:Ignorable="d"
        Title="AddAppointment" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="ComboBoxBarber" HorizontalAlignment="Left" Margin="99,65,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Barber" HorizontalAlignment="Left" Margin="40,63,0,0" VerticalAlignment="Top"/>
        <Label Content="Service" HorizontalAlignment="Left" Margin="40,108,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="ComboBoxService" HorizontalAlignment="Left" Margin="99,112,0,0" VerticalAlignment="Top" Width="120"/>
        <Calendar x:Name="CalendarVisitDate" HorizontalAlignment="Left" Margin="291,49,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="168" Width="187">
            <Calendar.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Calendar.RenderTransform>
        </Calendar>

    </Grid>
</Window>
